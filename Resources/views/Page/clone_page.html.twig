{% extends 'SonataAdminBundle:CRUD:base_edit.html.twig' %}

{% block title %}
    {{ "Clone \"%name%\" to a new url"|trans({'%name%': admin.toString(object) }, 'SonataAdminBundle') }}
{% endblock%}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/raindroppage/js/pageAdmin.js') }}" type="text/javascript"></script>
{% endblock javascripts %}

{% block stylesheets %}
