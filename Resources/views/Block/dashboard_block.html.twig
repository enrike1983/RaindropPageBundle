{% extends 'SonataBlockBundle:Block:block_base.html.twig' %}

{% block block %}
<table class="table table-bordered table-striped sonata-ba-list">
    <thead>
        <tr>
            <th colspan="3">{% block title %}{% endblock %}</th>
        </tr>
    </thead>

    <tbody>
        <tr>
            <td>
                {% block content %}{% endblock %}
            </td>
        </tr>
    </tbody>
</table>
{% endblock %}
